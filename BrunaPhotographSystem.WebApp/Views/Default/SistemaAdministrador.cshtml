
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="UTF-8">
    <link href="~/css/reset.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <link href="~/css/Responsivo.css" rel="stylesheet" type="text/css" />
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>SistemaAdministrador</title>
</head>
<body>
    <div class="container center-block" style="max-width:400px;max-height:400px;margin-top:10%;">
        <form controller="Cliente" action="Redirecionar" method="post">
            <button type="submit" style="float:left; max-width:150px;max-height:150px;display:block; margin:auto; padding:2em;margin-top:.4em;" name="controllerAction" value="Cliente/Index" class="btn btn-outline-primary center-block">Cadastro de Clientes</button>
        </form>
        <form controller="Foto" action="Redirecionar" method="post">
            <button type="submit" style="float:right; max-width:150px;max-height:150px;display:block; margin:auto; padding:2em;margin-top:.4em;" name="controllerAction" value="Foto/Index" class="btn btn-outline-primary center-block">Cadastro de Foto</button>
        </form>
        <form controller="Album" action="Redirecionar" method="post">
            <button type="submit" style="float:left; max-width:150px;max-height:150px;display:block; margin:auto; padding:2em;margin-top:.4em;" name="controllerAction" value="Album/Index" class="btn btn-outline-primary center-block">Cadastro de Album</button>
        </form>
        <form controller="Produto" action="Redirecionar" method="post">
            <button type="submit" style="float:right; max-width:150px;max-height:150px;display:block; margin:auto; padding:2em;margin-top:.4em;" name="controllerAction" value="Produto/Index" class="btn btn-outline-primary center-block">Cadastro de Produto</button>
        </form>
        <form controller="Default" action="VoltarAoSite" method="post">
            <button type="submit" style="float:right; max-width:150px;max-height:150px;display:block; margin:auto; padding:2em;margin-top:.4em;" class="btn btn-outline-primary center-block">Voltar ao Site</button>
        </form>
    </div>
    
</body>
</html>
